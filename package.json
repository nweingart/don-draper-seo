{
  "name": "don-draper-seo",
  "version": "1.0.0",
  "description": "Scan any website for common SEO issues and get a scored terminal report with actionable fixes",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "don-draper-seo": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && vite build",
    "build:cli": "tsup",
    "build:web": "vite build",
    "dev": "tsup --watch",
    "dev:web": "vite",
    "dev:server": "node dist/server/index.js"
  },
  "keywords": [
    "seo",
    "audit",
    "cli",
    "react",
    "meta-tags",
    "opengraph"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "hono": "^4.4.0",
    "@hono/node-server": "^1.11.0",
    "better-sqlite3": "^11.0.0",
    "ora": "^8.1.0"
  },
  "optionalDependencies": {
    "playwright": "^1.48.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/node": "^20.14.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.12.0",
    "tsup": "^8.1.0",
    "typescript": "^5.5.0",
    "vite": "^6.0.0"
  }
}
