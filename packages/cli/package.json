{
  "name": "don-draper-seo",
  "version": "1.0.1",
  "description": "Scan any website for common SEO issues and get a scored report with actionable fixes",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "don-draper-seo": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "seo",
    "audit",
    "cli",
    "meta-tags",
    "opengraph"
  ],
  "license": "MIT",
  "homepage": "https://nweingart.github.io/don-draper-seo/",
  "repository": {
    "type": "git",
    "url": "https://github.com/nweingart/don-draper-seo.git",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cheerio": "^1.0.0",
    "commander": "^12.1.0",
    "ora": "^8.1.0"
  },
  "optionalDependencies": {
    "playwright": "^1.48.0"
  },
  "devDependencies": {
    "tsup": "^8.1.0",
    "typescript": "^5.5.0",
    "@types/node": "^20.14.0"
  }
}
